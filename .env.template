# Takaro Astroneer Bridge Configuration
# Copy this file to .env and fill in your values

# Your game server name/identity (choose any unique name for your server)
# Example: IDENTITY_TOKEN=My Astroneer Server
IDENTITY_TOKEN=

# REQUIRED: Registration token from Takaro
# Get this from your Takaro dashboard when adding a new game server
REGISTRATION_TOKEN=

# HTTP API port for Lua mod to connect to (default: 3000)
HTTP_PORT=3000
